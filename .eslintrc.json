{
  "extends": "next",
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/no-unknown-property": "off",
    "@next/next/no-page-custom-font": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"]
}
